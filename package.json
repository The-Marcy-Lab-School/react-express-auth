{
  "name": "ls",
  "version": "1.0.0",
  "description": "This repo can be used to start a React+Express project fully equipped with Auth for user creation and login.",
  "main": "index.js",
  "scripts": {
    "kickstart": "cd frontend && npm i && cd .. && npm i && npm run migrate && npm run seed && npm run dev",
    "build": "npm i && && npm run migrate:rollback && npm run migrate && npm run seed",
    "start": "nodemon src/index.js",
    "dev": "nodemon ./src/index.js",
    "console": "node -r ./src/db/models/console.js",
    "connect": "psql -U postgres -d auth_example",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "play:bcrypt": "nodemon ./playground-examples/bcrypt-example.js",
    "lint": "eslint . --fix",
    "dev:frontend": "cd frontend && npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "agora-rtc-sdk-ng": "^4.18.2",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "connect-session-knex": "^3.0.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase": "^10.3.1",
    "knex": "^2.4.2",
    "openai": "^4.0.1",
    "pg": "^8.10.0",
    "react-chat-engine": "^1.11.28",
    "react-chat-engine-advanced": "^0.1.28",
    "react-chat-engine-pretty": "^0.1.8",
    "react-dropzone": "^14.2.3",
    "react-speech-recognition": "^3.10.0",
    "regenerator-runtime": "^0.14.0",
    "vite": "^2.9.16"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
